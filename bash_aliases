#aliases
alias ls='ls --color=always'
alias ll='ls -l'
alias grep='grep --color --exclude="*.min.js" --exclude-dir=Godeps --exclude-dir=vendor --exclude-dir=.git --exclude-dir=node_modules'
alias egrep='egrep --color'
alias ssh='ssh -q'
alias scp='scp -q'
alias screen='screen -d -R'
alias cls='clear'

